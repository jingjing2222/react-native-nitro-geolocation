{
  "name": "react-native-nitro-geolocation",
  "version": "0.2.0",
  "description": "âš¡ðŸš€Blazing-fast geolocation for React Native powered by Nitro Modules",
  "main": "src/index",
  "source": "src/index",
  "exports": {
    ".": {
      "types": "./src/index.tsx",
      "default": "./src/index.tsx"
    },
    "./compat": {
      "types": "./src/compat/index.tsx",
      "default": "./src/compat/index.tsx"
    }
  },
  "codegenConfig": {
    "name": "NitroGeolocationSpec",
    "type": "modules",
    "jsSrcsDir": "src"
  },
  "scripts": {
    "typecheck": "tsc --noEmit"
  },
  "author": "jingjing2222",
  "license": "MIT",
  "homepage": "https://react-native-nitro-geolocation.pages.dev",
  "repository": "https://github.com/jingjing2222/react-native-nitro-geolocation",
  "files": [
    "src",
    "react-native.config.js",
    "lib",
    "nitrogen",
    "cpp",
    "android/build.gradle",
    "android/fix-prefab.gradle",
    "android/gradle.properties",
    "android/CMakeLists.txt",
    "android/src/**/*",
    "ios/**/*.h",
    "ios/**/*.m",
    "ios/**/*.mm",
    "ios/**/*.cpp",
    "ios/**/*.swift",
    "app.plugin.js",
    "*.podspec",
    "README.md"
  ],
  "devDependencies": {
    "@react-native/babel-preset": "0.81.1",
    "@tsconfig/react-native": "^3.0.7",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^18.0.0",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-nitro-modules": "0.32.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.75.0",
    "react-native-nitro-modules": ">=0.32.0"
  },
  "create-react-native-library": {
    "languages": "kotlin-swift",
    "type": "nitro-module",
    "version": "0.54.3"
  }
}
